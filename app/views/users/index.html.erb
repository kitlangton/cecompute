<h2 class="small-12 medium-8 large-5 small-centered columns text-center">Users</h2>

<% @users.each do |user| %>
  <hr />
  <div class="user small-12 medium-8 large-5 small-centered columns text-center">
  <%= user.email  %>
  <div>
    <%= link_to "Edit", edit_user_path(user)  %>
    <%= link_to "Delete", user_path(user), method: :delete  %>
  </div>
  </div>
<% end %>
